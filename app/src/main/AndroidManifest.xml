<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gaidz.aplikasipakarkenakalanremaja">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_kuy"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_kuy"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplikasiPakarKenakalanRemaja">
        <activity android:name=".SolusiActivity"></activity>
        <activity android:name=".BantuanActivity" />
        <activity android:name=".TentangActivity" />
        <activity android:name=".HasilDiagnosaActivity" />
        <activity android:name=".DiagnosaActivity" />
        <activity android:name=".DetailKenakalanRemajaActivity" />
        <activity android:name=".KenakalanRemajaActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>